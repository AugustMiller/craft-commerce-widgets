{
    "name": "bymayo/commerce-widgets",
    "description": "Helpful widgets to use with Craft Commerce",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "commerce widgets"
    ],
    "support": {
        "docs": "https://github.com/bymayo/commerce-widgets/blob/master/README.md",
        "issues": "https://github.com/bymayo/commerce-widgets/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ByMayo",
            "homepage": "http://bymayo.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "bymayo\\commercewidgets\\": "src/"
        }
    },
    "extra": {
        "name": "Commerce Widgets",
        "handle": "commerce-widgets",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/bymayo/commerce-widgets/master/CHANGELOG.md",
        "components": {
            "commerceWidgetsService": "bymayo\\commercewidgets\\services\\CommerceWidgetsService"
        },
        "class": "bymayo\\commercewidgets\\CommerceWidgets"
    }
}
